<keymap version="1" name="Default copy" parent="$default">
  <action id="ActivateThemePreviewToolWindow">
    <keyboard-shortcut first-keystroke="alt t" />
  </action>
  <action id="CheckinProject" />
  <action id="EditorMatchBrace" />
  <action id="EditorSelectWord" />
  <action id="FileStructurePopup">
    <keyboard-shortcut first-keystroke="shift ctrl m" />
  </action>
  <action id="InsertLiveTemplate" />
  <action id="ReformatCode">
    <keyboard-shortcut first-keystroke="ctrl alt l" />
    <keyboard-shortcut first-keystroke="ctrl k" second-keystroke="ctrl d" />
  </action>
  <action id="VimGotoDeclaration">
    <keyboard-shortcut first-keystroke="ctrl j" />
  </action>
  <action id="VimWindowDown">
    <keyboard-shortcut first-keystroke="ctrl w" second-keystroke="j" />
  </action>
  <action id="VimWindowLeft">
    <keyboard-shortcut first-keystroke="ctrl w" second-keystroke="h" />
  </action>
  <action id="VimWindowRight">
    <keyboard-shortcut first-keystroke="ctrl w" second-keystroke="r" />
    <keyboard-shortcut first-keystroke="ctrl w" second-keystroke="l" />
  </action>
  <action id="VimWindowUp">
    <keyboard-shortcut first-keystroke="ctrl w" second-keystroke="k" />
  </action>
</keymap>