<application>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="15" />
    <option name="FONT_FAMILY" value="DejaVu Sans Mono" />
    <option name="USE_LIGATURES" value="true" />
  </component>
  <component name="EditorSettings">
    <option name="STRIP_TRAILING_SPACES" value="None" />
    <option name="SHOW_PARAMETER_NAME_HINTS" value="false" />
  </component>
  <component name="TodoConfiguration">
    <pattern icon="question" useCustomColors="true" pattern="(?&lt;=\W|^)(?&lt;TAG&gt;NOTICE)(\W|$)(.*)">
      <option name="FOREGROUND" value="bf55" />
      <option name="FONT_TYPE" value="3" />
      <option name="ERROR_STRIPE_COLOR" value="54aae3" />
    </pattern>
    <pattern pattern="(?&lt;=\W|^)(?&lt;TAG&gt;BUG)(\W|$)(.*)" />
    <pattern pattern="(?&lt;=\W|^)(?&lt;TAG&gt;TODO)(\W|$)(.*)" />
    <pattern pattern="\bNotImplementedException\b" />
  </component>
</application>